<script>
function normalizar(texto) {
  return texto
    .normalize("NFD")                // separa acentos
    .replace(/[\u0300-\u036f]/g, "") // remove acentos
    .trim()
    .toLowerCase();                  // força minúsculas
}

document.getElementById("produtos").addEventListener("change", function () {
  const selecionadoComAcento = this.options[this.selectedIndex].text.trim(); 


  const selecionado = this.value; // sem acento
  const dadosFiltrados = dados.filter(
    item => normalizar(item.tipo_produto) === normalizar(selecionado)
  );
  
  if (dadosFiltrados.length > 0) {
    const produtoInfo = dadosFiltrados[0]; // pega o primeiro objeto

