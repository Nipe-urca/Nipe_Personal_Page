<script>
document.getElementById("produtos").addEventListener("change", function () {
  const selecionado = this.value; // sem acento
  const dadosFiltrados = dados.filter(
    item => normalizar(item.tipo_produto) === normalizar(selecionado)
  );
  
  

  // Atualiza ou cria o texto informativo
  let novoTexto = document.getElementById("info-produto");
  if (!novoTexto) {
    novoTexto = document.createElement("p");
    novoTexto.id = "info-produto";
    document.querySelector(".relatorio").appendChild(novoTexto);
  }

  if (dadosFiltrados.length > 0) {
    const produto = dadosFiltrados[0];
    novoTexto.textContent = ``;
  } else {
    novoTexto.textContent = `Informações não encontradas para o produto selecionado.`;
  }

  // Prepara os dados para o gráfico
  const dias = dadosFiltrados.map(item => item.dia);
  const medias = dadosFiltrados.map(item => item.media);

  // Cria ou atualiza o gráfico
  const ctx = document.getElementById("graficoProduto").getContext("2d");

  // Destroi gráfico anterior se existir
  if (window.graficoInstancia) {
    window.graficoInstancia.destroy();
  }

  // Calcula os limites com folga
const minY = Math.min(...medias);
const maxY = Math.max(...medias);
const margem = (maxY - minY) * 0.1 || 1; // 10% de margem ou 1 se os valores forem iguais




// Calcula a média móvel de 7 períodos
function calcularMediaMovel(data, periodo) {
  const resultado = [];
  for (let i = 0; i < data.length; i++) {
    if (i < periodo - 1) {
      resultado.push(null); // Insere null para manter o alinhamento
    } else {
      const soma = data.slice(i - periodo + 1, i + 1).reduce((acc, val) => acc + val, 0);
      resultado.push(soma / periodo);
    }
  }
  return resultado;
}

if (dadosFiltrados.length >= 2) {
  const precoAtual = dadosFiltrados[dadosFiltrados.length - 1].media;
  const precoAnterior = dadosFiltrados[dadosFiltrados.length - 2].media;
  const variacao = ((precoAtual - precoAnterior) / precoAnterior) * 100;

  document.getElementById("preco-atual").textContent = `Preço atual: R$ ${precoAtual.toFixed(2)}`;
  document.getElementById("preco-anterior").textContent = `Preço anterior: R$ ${precoAnterior.toFixed(2)}`;
  document.getElementById("variacao").textContent = `Variação: ${variacao.toFixed(2)}%`;
} else {
  document.getElementById("preco-atual").textContent = `Preço atual: --`;
  document.getElementById("preco-anterior").textContent = `Preço anterior: --`;
  document.getElementById("variacao").textContent = `Variação: --`;
}

// Dentro do listener "change"
const mediasMovel = calcularMediaMovel(medias, 7);



// Elementos necessários para o gráfico de variações diárias
//-----------------------------------------------------------
// Calcula a variação percentual diária
function calcularVariacaoDiaria(medias) {
  const variacoes = [];
  for (let i = 0; i < medias.length; i++) {
    if (i === 0) {
      variacoes.push(null); // Sem variação no primeiro dia
    } else {
      const anterior = medias[i - 1];
      const atual = medias[i];
      const variacao = ((atual - anterior) / anterior) * 100;
      variacoes.push(variacao.toFixed(2));
    }
  }
  return variacoes;
}

const variacoesDiarias = calcularVariacaoDiaria(medias);
//-----------------------------------------------------------


// Elementos necessários para o gráfico de desvios
//-----------------------------------------------------------
// Média histórica
const mediaHistorica = medias.reduce((acc, val) => acc + val, 0) / medias.length;

// Desvio em relação à média histórica
const desviosHistorico = medias.map(valor => (valor - mediaHistorica).toFixed(2));

// Desvio em relação à média móvel de 7 dias
const desviosMovel = medias.map((valor, i) => {
  const mediaMovel = mediasMovel[i];
  return mediaMovel !== null ? (valor - mediaMovel).toFixed(2) : null;
});
//-----------------------------------------------------------



// Elementos necessários para o gráfico de desvios para os extremos
//-----------------------------------------------------------
// Mínima e máxima histórica
const minHistorica = Math.min(...medias);
const maxHistorica = Math.max(...medias);

// Desvios em relação à mínima e máxima histórica
const desviosMinHistorica = medias.map(valor => (valor - minHistorica).toFixed(2));
const desviosMaxHistorica = medias.map(valor => (valor - maxHistorica).toFixed(2));

// Desvios em relação à mínima e máxima dos últimos 30 dias
const desviosMin30 = [];
const desviosMax30 = [];

for (let i = 0; i < medias.length; i++) {
  const inicio = Math.max(0, i - 29);
  const janela = medias.slice(inicio, i + 1);
  const min30 = Math.min(...janela);
  const max30 = Math.max(...janela);

  desviosMin30.push((medias[i] - min30).toFixed(2));
  desviosMax30.push((medias[i] - max30).toFixed(2));
}
//-----------------------------------------------------------


// Destroi gráfico anterior se existir
if (window.graficoDesviosExtremosInstancia) {
  window.graficoDesviosExtremosInstancia.destroy();
}

const ctxDesviosExtremos = document.getElementById("graficoDesviosExtremos").getContext("2d");

window.graficoDesviosExtremosInstancia = new Chart(ctxDesviosExtremos, {
  type: "line",
  data: {
    labels: dias,
    datasets: [
      {
        label: "Desvio vs Mínima Histórica",
        data: desviosMinHistorica,
        borderColor: "rgba(255, 99, 132, 1)",
        backgroundColor: "rgba(255, 99, 132, 0.2)",
        fill: false,
        tension: 0.3
      },
      {
        label: "Desvio vs Máxima Histórica",
        data: desviosMaxHistorica,
        borderColor: "rgba(54, 162, 235, 1)",
        backgroundColor: "rgba(54, 162, 235, 0.2)",
        fill: false,
        borderDash: [4, 4],
        tension: 0.3
      },
      {
        label: "Desvio vs Máxima (últimos 30 dias)",
        data: desviosMax30,
        borderColor: "rgba(255, 206, 86, 1)",
        backgroundColor: "rgba(255, 206, 86, 0.2)",
        fill: false,
        tension: 0.3
      },
      {
        label: "Desvio vs Mínima (últimos 30 dias)",
        data: desviosMin30,
        borderColor: "rgba(75, 192, 192, 1)",
        backgroundColor: "rgba(75, 192, 192, 0.2)",
        fill: false,
        borderDash: [2, 2],
        tension: 0.3
      }
    ]
  },
  options: {
    responsive: true,
    plugins: {
      title: {
        display: true,
        text: `Desvios para os extremos da série - ${selecionado.toLowerCase()}`,
        font: {
          size: 16
        }
      }
    },
    scales: {
      y: {
        title: {
          display: true,
          text: "Desvio (R$)"
        },
        beginAtZero: false
      },
      x: {
        title: {
          display: true,
          text: "Dia"
        }
      }
    }
  }
});


// Destroi gráfico anterior de variação diária se existir
if (window.graficoVariacaoDiariaInstancia) {
  window.graficoVariacaoDiariaInstancia.destroy();
}

const ctxVarDia = document.getElementById("graficoVariacaoDiaria").getContext("2d");

window.graficoVariacaoDiariaInstancia = new Chart(ctxVarDia, {
  type: "bar",
  data: {
    labels: dias,
    datasets: [{
      label: "Variação diária (%)",
      data: variacoesDiarias,
      backgroundColor: variacoesDiarias.map(v => v >= 0 ? "rgba(75, 192, 192, 0.6)" : "rgba(255, 99, 132, 0.6)"),
      borderColor: variacoesDiarias.map(v => v >= 0 ? "rgba(75, 192, 192, 1)" : "rgba(255, 99, 132, 1)"),
      borderWidth: 1
    }]
  },
  options: {
    responsive: true,
    plugins: {
      title: {
        display: true,
        text: `Variação percentual - ${selecionado.toLowerCase()}`,
