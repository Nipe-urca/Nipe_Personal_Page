<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<canvas id="graficoIndice" width="600" height="400"></canvas>

<script>
  const dias = indice.map(item => item.dia);
  const deltaF = indice.map(item => item.deltaF ?? 0);
  const deltaH = indice.map(item => item.deltaH ?? 0);
  const deltaIndex = indice.map(item => item.deltaIndex ?? 0);

  const ctx = document.getElementById("graficoIndice").getContext("2d");
  const graficoIndice = new Chart(ctx, {
    type: "line",
    data: {
      labels: dias,
      datasets: [
        {
          label: "IPPAC Frutas",
          data: deltaF,
          borderColor: "rgba(255, 99, 132, 1)",
          backgroundColor: "rgba(255, 99, 132, 0.2)",
          fill: false,
          tension: 0.3
        },
        {
          label: "IPPAC Hortaliças",
          data: deltaH,
          borderColor: "rgba(54, 162, 235, 1)",
          backgroundColor: "rgba(54, 162, 235, 0.2)",
          fill: false,
          tension: 0.3
        },
        {
          label: "IPPAC Geral",
          data: deltaIndex,
          borderColor: "rgba(75, 192, 192, 1)",
          backgroundColor: "rgba(75, 192, 192, 0.2)",
          fill: false,
          tension: 0.3
        }
      ]
    },
    options: {
      responsive: true,
      plugins: {
        title: {
          display: true,
          text: "Variações temporais (IPPAC Frutas, IPPAC Hortaliças, IPPAC Geral)"
        },
        legend: {
          position: "top"
        }
      },
      scales: {
        x: {
          title: {
            display: true,
            text: "Dia"
          }
        },
        y: {
          title: {
            display: true,
            text: "Variação (%)"
          }
        }
      }
    }
  });
</script>


